{
  "name": "react-boilerplate",
  "version": "1.1.0",
  "main": "bin/www",
  "license": "GPL-3.0",
  "scripts": {
    "start": "node bin/www",
    "dev": "nodemon bin/www",
    "build-prod": "webpack --mode=production",
    "watch": "webpack -w --mode=development",
    "clean": "rm -rf .git",
    "postinstall": "webpack --mode=production",
    "lint": "eslint app.js bin/www src/**/*.js src/**/*.js",
    "gen-ssl": "openssl req -nodes -x509 -newkey rsa:4096 -keyout server-key.pem -out server-cert.pem -days 365",
    "setup": "yarn; yarn clean; yarn gen-ssl"
  },
  "dependencies": {
    "@babel/core": "^7.0.0-beta.38",
    "@babel/polyfill": "^7.0.0-beta.38",
    "@babel/preset-env": "^7.0.0-beta.38",
    "@babel/preset-react": "^7.0.0-beta.38",
    "autoprefixer": "^7.2.6",
    "babel-loader": "8.0.0-beta.0",
    "body-parser": "^1.18.2",
    "bootstrap": "^4.0.0",
    "chalk": "^2.3.1",
    "compression": "^1.7.1",
    "css-loader": "^0.28.7",
    "express": "^4.15.4",
    "express-force-ssl": "^0.3.2",
    "helmet": "^3.9.0",
    "local-env-var": "^2.0.4",
    "netjet": "^1.3.0",
    "postcss-loader": "^2.1.0",
    "prepack-webpack-plugin": "^1.1.0",
    "react": "^16.0.0",
    "react-dom": "^16.0.0",
    "react-redux": "^5.0.7",
    "react-router-dom": "^4.2.2",
    "redux": "^3.7.2",
    "socket.io": "^2.0.4",
    "source-map": "^0.6.1",
    "spdy": "^3.4.7",
    "style-loader": "^0.19.1",
    "webpack": "^4",
    "webpack-build-notifier": "^0.1.16",
    "webpack-cli": "^2.0.13",
    "winston": "^2.4.0"
  },
  "devDependencies": {
    "eslint": "^4.17.0",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.6.1",
    "nodemon": "^1.14.12"
  }
}
